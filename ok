                    If matrizaux1(18) = 10 Then
                        suma = suma + matrizaux1(20)
                        i = i + 1
                        matrizaux2(9) = suma
                        TextBox31.text = matrizaux2(9)
                        'suma = suma + matrizaux1(19) + matrizaux1(20)
                    Else
                        suma = suma + matrizaux1(20)
                        matrizaux2(9) = suma
                        TextBox31.text = matrizaux2(9)
                        i = i + 1
                    End If
                Else
                    If a = 1 Then
                        suma = suma + matrizaux1(20)
                        i = i + 1
                        matrizaux2(9) = suma
                        TextBox31.text = matrizaux2(9)
                        'suma = suma + matrizaux1(20)
                    Else
                        If a = 2 Then
                            ' suma = suma + matrizaux1(19)
                            i = i + 1
                        End If
                    End If
                End If
                x = 1
            End If
        End If

        label3.Text = TextBox31.Text

    End Function





End Module
