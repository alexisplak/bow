Public Class Form1
    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        band = 1

        jugador = 1
        jugar = 1
    End Sub

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click

        Do While jugar < 10
            If jugador = 1 Then
                Call transferecia(matriz_a, matriz_b)

                jugar = 1
                Exit Do
            End If

        Loop

        Do While band < 20 Or band1 < 20

            If band = 1 Then

                Call tiro1(TextBox3, TextBox60, matrizaux1, matrizaux2)

                band = band + 1

                Exit Do
            End If

            If band = 2 Then
                Call tiro2(TextBox3, TextBox60, matrizaux1, matrizaux2)

                band = band + 1

                If matriz_auxA(2) = 10 Then

                    band = band + 1

                End If
                Exit Do
            End If

            If band = 3 Then
                Call tiro3(TextBox3, TextBox6, TextBox57, TextBox60, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do

            End If

            If band = 4 Then
                Call tiro4(TextBox3, TextBox6, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(4) = 10 Then
                    band = band + 1
                End If

                Exit Do
            End If

            If band = 5 Then
                Call tiro5(TextBox6, TextBox9, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 6 Then
                Call tiro6(TextBox6, TextBox9, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(6) = 10 Then
                    band = band + 1
                End If

                Exit Do
            End If

            If band = 7 Then
                Call tiro7(TextBox9, TextBox12, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 8 Then
                Call tiro8(TextBox9, TextBox12, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(8) = 10 Then
                    band = band + 1
                End If
                Exit Do
            End If

            If band = 9 Then
                Call tiro9(TextBox12, TextBox15, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 10 Then
                Call tiro10(TextBox12, TextBox15, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(10) = 10 Then
                    band = band + 1
                End If
                Exit Do
            End If

            If band = 11 Then
                Call tiro11(TextBox15, TextBox18, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 12 Then
                Call tiro12(TextBox15, TextBox18, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(12) = 10 Then
                    band = band + 1
                End If
                Exit Do
            End If

            If band = 13 Then
                Call tiro13(TextBox18, TextBox21, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 14 Then
                Call tiro14(TextBox18, TextBox21, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(14) = 10 Then
                    band = band + 1
                End If
                Exit Do
            End If

            If band = 15 Then
                Call tiro15(TextBox21, TextBox24, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 16 Then
                Call tiro16(TextBox21, TextBox24, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(16) = 10 Then
                    band = band + 1
                End If
                Exit Do
            End If

            If band = 17 Then
                Call tiro17(TextBox24, TextBox27, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 18 Then
                Call tiro18(TextBox24, TextBox27, matrizaux1, matrizaux2)

                band = band + 1
                Exit Do
            End If

            If band = 19 Then
                Call tiro19(TextBox25, TextBox27, TextBox31, matrizaux1, matrizaux2)

                band = band + 1
                If matriz_a(18) + matriz_a(19) < 10 Then
                    band = band + 1
                End If

                Exit Do
            End If

            If band = 20 Then
                Call tiro20(Label3, TextBox31, matrizaux1, matrizaux2)
                band = band + 1

                Exit Do
            End If

            Exit Do

        Loop

    End Sub


