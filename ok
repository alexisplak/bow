Module Module1
    Public a As Byte
    Public i As Byte
    Public x As Byte
    Public z As Byte
    Public n1 As String
    Public n2 As String
    Public band As Byte
    Public band1 As Byte
    Public suma As Integer
    Public suma1 As Integer
    Public jugador As Byte
    Public jugar As Byte
    Public texto As TextBox


    Public formulario As Form




    Public matrizaux1(20) As Byte
    Public matrizaux2(9) As Byte
    Public matriz_auxA(20) As Byte
    Public matriz_auxB(9) As Byte


    Public matriz_a(20) As Byte
    Public matriz_b(9) As Byte
    Public matriz_c(20) As Byte
    Public matriz_d(9) As Byte
    Public Tiro(20) As Byte

    Function flechas()

    End Function


    Function transferecia(matriz_auxA, matriz_auxB)
        Dim q As Byte


        For q = 0 To 20
            matrizaux1(q) = matriz_auxA(q)
        Next

        For q = 0 To 9
            matrizaux2(q) = matriz_auxB(q)

        Next


    End Function

    Function tiro1(Textbox3, textbox60, matrizaux1, matrizaux2)


        i = 0
            suma = 0
            a = 0



        If matrizaux1(0) = 10 Then
            suma = suma + matrizaux1(0)
            matrizaux1(1) = 0
            i = i + 3
            a = 0

        Else

            If matrizaux1(0) + matrizaux1(1) = 10 Then

                suma = suma + matrizaux1(0)

                    i = i + 3
                a = 1

            Else

                suma = suma + matrizaux1(0) + matrizaux1(1)
                matrizaux2(0) = suma
                Textbox3.text = matrizaux2(0)
                i = i + 3
                a = 2
            End If
        End If

    End Function

    Function tiro2(TextBox3, textbox60, matrizaux1, matrizaux2)

        If matrizaux1(2) = 10 Then
            If a = 0 Then
                suma = suma + matrizaux1(2)
                matrizaux1(3) = 0
                i = i + 2
                a = 0

            Else
                If a = 1 Then
                    suma = suma + matrizaux1(2)
                    matrizaux2(0) = suma
                    TextBox3.text = matrizaux2(0)
                    suma = suma + matrizaux1(2)
                    i = i + 2
                    a = 0

                Else
                    suma = suma + matrizaux1(2)
                    i = i + 2
                    a = 0

                End If
            End If
        End If

        If matrizaux1(2) < 10 Then

            If a = 0 Or a = 2 Then
                suma = suma + matrizaux1(2)
                i = i + 1
            End If

            If a = 1 Then
                suma = suma + matrizaux1(2)
                matrizaux2(0) = suma
                TextBox3.text = matrizaux2(0)
                i = i + 1
            End If
        End If


    End Function


    Function tiro3(TextBox3, TextBox6, textbox57, textbox60, matrizaux1, matrizaux2)


        If matrizaux1(2) + matrizaux1(3) < 10 Then

            If a = 0 Then

                suma = suma + matrizaux1(3)
                matrizaux2(0) = suma
                TextBox3.text = matrizaux2(0)
                suma = suma + matrizaux1(2) + matrizaux1(3)
                matrizaux2(1) = suma
                TextBox6.text = matrizaux2(1)
                a = 2
                i = i + 1
            Else


                If a = 2 Then
                    suma = suma + matrizaux1(3)
                    matrizaux2(1) = suma
                    TextBox6.text = matrizaux2(1)
                    a = 2
                    i = i + 1
                Else

                    If a = 1 Then
                        suma = suma + matrizaux1(2) + matrizaux1(3)
                        matrizaux2(1) = suma
                        TextBox6.text = matrizaux2(1)
                        a = 2
                        i = i + 1
                    End If
                End If
            End If
        End If

        If matrizaux1(2) + matrizaux1(3) = 10 Then
            If a = 0 Then
                a = 1
                i = i + 1
                suma = suma + matrizaux1(3)
                matrizaux2(0) = suma
                TextBox3.text = matrizaux2(0)
                suma = suma + 10

            Else
                If a = 1 Then
                    i = i + 1
                    a = 1
                    suma = suma + matrizaux1(3) + matrizaux1(2)

                Else
                    If a = 2 Then
                        a = 1
                        i = i + 1
                        suma = suma + matrizaux1(3)
                    End If
                End If
            End If
        End If



    End Function

    Function tiro4(TextBox3, TextBox6, matrizaux1, matrizaux2)


        If matrizaux1(4) = 10 Then

            If a = 0 Then
                If matriz_a(0) = 10 Then
                    suma = suma + 10
                    i = i + 2
                    matrizaux2(0) = suma
                    TextBox3.text = matrizaux2(0)
                    suma = suma + 20
                    matrizaux1(5) = 0
                Else
                    suma = suma + matrizaux1(4)
                    i = i + 2
                    matrizaux1(5) = 0
                End If
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(4)
                    i = i + 2
                    matrizaux2(1) = suma
                    TextBox6.text = matrizaux2(1)
                    suma = suma + matrizaux1(4)
                    matrizaux1(5) = 0
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(4)
                        i = i + 2
                        matrizaux1(5) = 0
                    End If
                End If
            End If
            a = 0
        End If

        If matrizaux1(4) < 10 Then
            If a = 0 Then
                If matrizaux1(0) = 10 Then
                    suma = suma + matrizaux1(4)
                    i = i + 1
                    matrizaux2(0) = suma
                    TextBox3.text = matrizaux2(0)
                    suma = suma + matriz_a(2) + matrizaux1(4)
                Else
                    suma = suma + matrizaux1(4)
                    i = i + 1
                End If
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(4)
                    i = i + 1
                    matrizaux2(1) = suma
                    TextBox6.text = matrizaux2(1)
                    suma = suma + matrizaux1(4)
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(4)
                        i = i + 1
                    End If
                End If
            End If
        End If

    End Function

    Function tiro5(TextBox6, TextBox9, matrizaux1, matrizaux2)

        If matrizaux1(4) + matrizaux1(5) = 10 Then
            If a = 0 Then
                suma = suma + matrizaux1(5)
                matrizaux2(1) = suma
                TextBox6.text = matrizaux2(1)
                suma = suma + matrizaux1(4) + matrizaux1(5)
                i = i + 1
                a = 1
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(5)
                    a = 1
                    i = i + 1
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(5)
                        a = 1
                        i = i + 1
                    End If
                End If
            End If
        End If

        If matrizaux1(4) + matrizaux1(5) < 10 Then
            If a = 0 Then
                suma = suma + matrizaux1(5)
                matrizaux2(1) = suma
                TextBox6.text = matrizaux2(1)
                suma = suma + matrizaux1(4) + matrizaux1(5)
                matrizaux2(2) = suma
                TextBox9.text = matrizaux2(2)
                i = i + 1
                a = 2
            Else

                If a = 1 Or a = 2 Then
                    suma = suma + matrizaux1(5)
                    matrizaux2(2) = suma
                    TextBox9.text = matrizaux2(2)
                    i = i + 1
                    a = 2

                End If
            End If
        End If


    End Function


    Function tiro6(TextBox6, TextBox9, matrizaux1, matrizaux2)

        If matrizaux1(6) = 10 Then

            If a = 0 Then
                If matrizaux1(2) = 10 Then
                    suma = suma + 10
                    i = i + 2
                    matrizaux2(1) = suma
                    TextBox6.text = matrizaux2(1)
                    suma = suma + 20
                    matrizaux1(7) = 0
                Else
                    suma = suma + matrizaux1(6)
                    i = i + 2
                    matrizaux1(7) = 0
                End If
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(6)
                    i = i + 2
                    matrizaux2(2) = suma
                    TextBox9.text = matrizaux2(2)
                    suma = suma + matrizaux1(6)
                    matrizaux1(7) = 0
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(6)
                        i = i + 2
                        matrizaux1(7) = 0
                    End If
                End If
            End If
            a = 0
        End If

        If matrizaux1(6) < 10 Then
            If a = 0 Then
                If matrizaux1(2) = 10 Then
                    suma = suma + matrizaux1(6)
                    i = i + 1
                    matrizaux2(1) = suma
                    TextBox6.text = matrizaux2(1)
                    suma = suma + matrizaux1(4) + matrizaux1(6)
                Else
                    suma = suma + matrizaux1(6)
                    i = i + 1
                End If
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(6)
                    i = i + 1
                    matrizaux2(2) = suma
                    TextBox9.text = matrizaux2(2)
                    suma = suma + matrizaux1(6)
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(6)
                        i = i + 1
                    End If
                End If
            End If
        End If



    End Function

    Function tiro7(TextBox9, TextBox12, matrizaux1, matrizaux2)

        If matrizaux1(6) + matrizaux1(7) = 10 Then
            If a = 0 Then
                suma = suma + matrizaux1(7)
                matrizaux2(2) = suma
                TextBox9.text = matrizaux2(2)
                suma = suma + matrizaux1(6) + matrizaux1(7)
                i = i + 1
                a = 1
            Else
                If a = 1 Then
                    suma = suma + matrizaux1(7)
                    a = 1
                    i = i + 1
                Else
                    If a = 2 Then
                        suma = suma + matrizaux1(7)
                        a = 1
                        i = i + 1
                    End If
                End If
            End If
        End If

        If matrizaux1(6) + matrizaux1(7) < 10 Then
            If a = 0 Then
                suma = suma + matrizaux1(7)
                matrizaux2(2) = suma
                TextBox9.text = matrizaux2(2)
                suma = suma + matrizaux1(6) + matrizaux1(7)
